{
  "mcpServers": {
    "exa": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.exa.ai/mcp"
      ],
      "env": {
          "EXA_API_KEY": "${exa.api.key}"
      }
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp", "--api-key", "${context7.api.key}"]
    },
    "met-museum": {
      "command": "npx",
      "args": [
        "-y",
        "metmuseum-mcp"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--isolated"
      ]
    },
    "mcp-server-chart": {
      "command": "npx",
      "args": ["-y", "@antv/mcp-server-chart"]
    },
    "coingecko_mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://mcp.api.coingecko.com/mcp"
      ]
    },
    "airbnb": {
      "command": "npx",
      "args": [
        "-y",
        "@openbnb/mcp-server-airbnb"
      ]
    },
    "mcp-time": {
      "command": "npx",
      "args": [
        "-y",
        "@theo.foobar/mcp-time"
      ]
    },
    "alphavantage": {
      "command": "uvx",
      "args": [
        "av-mcp",
        "${alphavantage.api.key}"
      ]
    },
    "campground-search-mcp-server": {
      "command": "npx",
      "args": ["-y", "campertunity-mcp-server@latest"],
      "env": {
        "CAMPERTUNITY_API_KEY": "${campertunity.api.key}"
      }
    }
  }
}